{
  "$schema": "https://turbo.build/schema.json",
  "globalPassThroughEnv": [
    "CI",
    "COREPACK_INTEGRITY_KEYS",
    "COREPACK_NPM_REGISTRY",
    "COREPACK_DEFAULT_TO_LATEST",
    "NODE_OPTIONS"
  ],
  "ui": "tui",
  "tasks": {
    "api-extractor": {
      "dependsOn": [
        "//#build"
      ],
      "cache": false
    },
    "build": {
      "dependsOn": [
        "//#build",
        "^build"
      ]
    },
    "test": {
      "dependsOn": [],
      "cache": false
    },
    "//#build": {
      "cache": false,
      "dependsOn": [],
      "inputs": [
        "**/*.{ts,tsx}",
        ".github/**",
        "!node_modules/**",
        "!**/*.d.ts"
      ]
    }
  }
}
